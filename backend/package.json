{
  "name": "zenith-pdf-backend",
  "version": "2.0.0",
  "description": "Zenith PDF Backend - Real-time collaborative PDF editor",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --clear-screen=false src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "node --loader tsx src/database/migrate.ts",
    "seed": "node --loader tsx src/database/seed.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/redis": "^6.1.1",
    "@fastify/websocket": "^10.0.1",
    "@aws-sdk/client-s3": "^3.509.0",
    "@aws-sdk/s3-request-presigner": "^3.509.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "pdf-lib": "^1.17.1",
    "pg": "^8.11.3",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.11",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
